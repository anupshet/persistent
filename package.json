  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --o",
    "build": "ng build br-component-library && ng build",
    "patch": "patch-package",
    "build-prod": "ng build br-component-library && ng build --prod --output-hashing=all --progress=true --verbose=true && npm run post-build",
    "build-lib": "ng build br-component-library",
    "test-lib": "ng test br-component-library",
    "test-app": "ng test unext-app --browsers=Chrome",
    "test": "ng test --browsers=Chrome",
    "posttest": "allure generate allure-results --clean -o allure-report || true",
    "test-single-headless": "ng test br-component-library --watch=false --browsers=ChromeHeadless --reporters=progress --progress --source-map=true && ng test unext-app --watch=false --browsers=ChromeHeadless --reporters=progress --progress --source-map=true",
    "test-single-headless-CI": "ng test br-component-library --watch=false --browsers=ChromeHeadless --progress --source-map=false && ng test unext-app --watch=false --browsers=ChromeHeadless --progress --source-map=false",
    "lint": "ng lint",
    "pree2e": "webdriver-manager --ignore_ssl update --ie --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js",
    "e2e-ie": "protractor ./protractor-ie.conf.js",
    "e2e-ed": "protractor ./protractor-ed.conf.js",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "build-web-config": "copyfiles --flat src/web.config dist",
    "post-build": "node ./build/post-build.js",
    "json-server": "json-server --watch db.json"
  },
