{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "unext-app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/ngx-extended-pdf-viewer/assets/",
                "output": "/assets/"
              },
              "src/config",
              "src/favicon.ico",
              "src/web.config"
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/ng-busy/src/style/busy.css"
            ],
            "scripts": [
              "src/assets/js/awssign.js",
              "node_modules/powerbi-client/dist/powerbi.min.js",
              "node_modules/powerbi-models/dist/models.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "en-US": {
              "aot": true,
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "150kb"
                }
              ],
              "localize": [
                "en-US"
              ],
              "outputHashing": "all",
              "outputPath": "dist"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "unext-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "unext-app:build:production"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/styles.scss",
              "node_modules/ng-busy/src/style/busy.css"
            ],
            "assets": [
              "src/assets",
              "src/config",
              "src/favicon.ico",
              "src/web.config"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "unext-app-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "unext-app:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "br-component-library": {
      "root": "projects/br-component-library",
      "sourceRoot": "projects/br-component-library/src",
      "projectType": "library",
      "prefix": "br",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/br-component-library/tsconfig.lib.json",
            "project": "projects/br-component-library/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/br-component-library/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/br-component-library/src/test.ts",
            "polyfills": "projects/br-component-library/src/polyfills.ts",
            "tsConfig": "projects/br-component-library/tsconfig.spec.json",
            "karmaConfig": "projects/br-component-library/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/br-component-library/tsconfig.lib.json",
              "projects/br-component-library/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "unext-app",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "unext",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "unext"
    }
  },
  "cli": {
    "analytics": false
  }
}
